.schema
drop table Dreams;
CREATE TABLE Businesses (
id int,
name text,
address text,
description text,
tags text
, zipcode text, phone text, email text, url text, covidRestrictions text, schedule text, lat real, long real);
CREATE TABLE Users (
id int,
username text,
hash text,
tokens text
);
CREATE TABLE Analytics (
id int,
priceMin real,
priceMax real,
priceAvg real,
clickCount int
, custPriceAvg real);
CREATE TABLE Feedback (
id int,
obj text,
rating real,
price real
, email text, ip text);
.quit
select * from businesses
;
select * from feedback
;
delete from feedback
;
select * from users
;
.quit
